{
	"name": "server",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "biome check --write",
		"build": "nest build",
		"start": "cross-env NODE_ENV=production nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "cross-env NODE_ENV=production node dist/main",
		"migration:generate": "pnpm build && typeorm migration:generate -o -d ./dist/typeorm/data-source ./migrations/migration",
		"migration:run": "typeorm migration:run -d ./dist/typeorm/data-source",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@blazity/nest-file-fastify": "1.0.0",
		"@fastify/multipart": "8.3.0",
		"@fastify/static": "7.0.4",
		"@mapbox/polyline": "1.2.1",
		"@nestjs-modules/mailer": "2.0.2",
		"@nestjs/common": "10.4.0",
		"@nestjs/config": "3.2.3",
		"@nestjs/core": "10.4.0",
		"@nestjs/jwt": "10.2.0",
		"@nestjs/platform-fastify": "10.4.0",
		"@nestjs/swagger": "7.4.0",
		"@nestjs/typeorm": "10.0.2",
		"bcrypt": "5.1.1",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.1",
		"dotenv": "16.4.5",
		"fastify": "4.28.1",
		"luxon": "3.5.0",
		"mjml": "4.15.3",
		"node-mime-types": "1.1.2",
		"nodemailer": "6.9.14",
		"pg": "8.12.0",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.1",
		"ts-pattern": "^5.3.1",
		"ts-xor": "1.3.0",
		"typeorm": "0.3.20",
		"typeorm-naming-strategies": "4.1.0"
	},
	"devDependencies": {
		"@nestjs/cli": "10.4.4",
		"@nestjs/schematics": "10.1.3",
		"@nestjs/testing": "10.4.0",
		"@swc/cli": "0.4.0",
		"@swc/core": "1.7.10",
		"@types/bcrypt": "5.0.2",
		"@types/jest": "29.5.12",
		"@types/luxon": "3.4.2",
		"@types/mapbox__polyline": "1.0.5",
		"@types/mjml": "4.7.4",
		"@types/mjml-core": "4.15.0",
		"@types/node": "20.14.15",
		"@types/nodemailer": "6.4.15",
		"@types/supertest": "6.0.2",
		"@typescript-eslint/eslint-plugin": "7.0.0",
		"@typescript-eslint/parser": "7.0.0",
		"cross-env": "7.0.3",
		"jest": "29.7.0",
		"source-map-support": "0.5.21",
		"supertest": "7.0.0",
		"ts-jest": "29.2.4",
		"ts-loader": "9.5.1",
		"ts-node": "10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.5.4"
	},
	"jest": {
		"moduleFileExtensions": ["js", "json", "ts"],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": ["**/*.(t|j)s"],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
